{
  "builds": [
    {
      "src": "Fantasy-Premier-League/main.py",
      "use": "@vercel/python",
      "config": {
        "requirementsPath": "Fantasy-Premier-League/requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/Fantasy-Premier-League/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Fantasy-Premier-League/main.py"
    }
  ]
}
